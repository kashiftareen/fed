a=int(input("Enter first number here:"))
c=(input("Enter operator +,-,*,/,**,//:"))
b=int(input("Enter second number here:"))
if c== "+":
    print(a+b)
if c=="-":
    print(a-b)
if c=="*":
    print(a*b)
if c=="**":
    print(a**b)
if c=="/":
    print(a/b)
if c=="//":
    print(a//b)
else:
    print("Final Result")  
    
   
